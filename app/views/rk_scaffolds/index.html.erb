<p id="notice"><%= notice %></p>

<h1>Listing Rk Scaffolds</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rk_scaffolds.each do |rk_scaffold| %>
      <tr>
        <td><%= link_to 'Show', rk_scaffold %></td>
        <td><%= link_to 'Edit', edit_rk_scaffold_path(rk_scaffold) %></td>
        <td><%= link_to 'Destroy', rk_scaffold, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rk scaffold', new_rk_scaffold_path %>
